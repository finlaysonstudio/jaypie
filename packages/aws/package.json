{
  "name": "@jaypie/aws",
  "version": "1.2.0-rc.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/finlaysonstudio/jaypie"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "lint": "eslint .",
    "test": "vitest run .",
    "test:spec:getMessages.function": "vitest run ./src/__tests__/getMessages.function.spec.ts",
    "test:spec:getSecret.function": "vitest run ./src/__tests__/getSecret.function.spec.ts",
    "test:spec:getSingletonMessage.function": "vitest run ./src/__tests__/getSingletonMessage.function.spec.ts",
    "test:spec:getTextractJob.function": "vitest run ./src/__tests__/getTextractJob.function.spec.ts",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.ts",
    "test:spec:sendBatchMessages.function": "vitest run ./src/__tests__/sendBatchMessages.function.spec.ts",
    "test:spec:sendMessage.function": "vitest run ./src/__tests__/sendMessage.function.spec.ts",
    "test:spec:sendTextractJob.function": "vitest run ./src/__tests__/sendTextractJob.function.spec.ts",
    "test:spec:validateQueueUrl.util": "vitest run ./src/__tests__/validateQueueUrl.util.spec.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.931.0",
    "@aws-sdk/client-sqs": "^3.540.0",
    "@aws-sdk/client-textract": "^3.726.1",
    "@aws-sdk/middleware-retry": "^3.370.0",
    "async-retry": "^1.3.3",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/async-retry": "^1.4.9",
    "@types/node": "^22.13.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
