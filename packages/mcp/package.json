{
  "name": "@jaypie/mcp",
  "version": "0.7.9",
  "description": "Jaypie MCP",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finlaysonstudio/jaypie.git"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./suite": {
      "types": "./dist/suite.d.ts",
      "import": "./dist/suite.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jaypie-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "release-notes",
    "skills"
  ],
  "scripts": {
    "build": "rollup --config && chmod +x dist/index.js",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "npm run build",
    "test": "vitest run .",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jaypie/fabric": "*",
    "@jaypie/llm": "*",
    "@jaypie/tildeskill": "*",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "commander": "^14.0.0",
    "gray-matter": "^4.0.3",
    "semver": "^7.7.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/express": "^5.0.3",
    "@types/semver": "^7.7.1",
    "express": "^5.1.0",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
