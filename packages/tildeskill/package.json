  "dependencies": {
    "@jaypie/errors": "*",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.1",
    "rollup": "^4.12.0",
    "typescript": "^5.3.3",
