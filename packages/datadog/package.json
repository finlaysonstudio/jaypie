{
  "name": "@jaypie/datadog",
  "version": "1.1.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/finlaysonstudio/jaypie"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "lint": "eslint .",
    "test": "vitest run .",
    "test:spec:constants": "vitest run ./src/__tests__/constants.spec.ts",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.ts",
    "test:spec:objectToKeyValueArray.pipeline": "vitest run ./src/__tests__/objectToKeyValueArray.pipeline.spec.ts",
    "test:spec:submitMetric.adapter": "vitest run ./src/__tests__/submitMetric.adapter.spec.ts",
    "test:spec:submitMetricSet.adapter": "vitest run ./src/__tests__/submitMetricSet.adapter.spec.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@datadog/datadog-api-client": "^1.24.0",
    "@jaypie/aws": "^1.1.0",
    "@jaypie/core": "^1.1.17",
    "hot-shots": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
