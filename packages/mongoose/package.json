{
  "name": "@jaypie/mongoose",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Finlayson Studio",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": {
        "require": "./dist/jaypie-mongoose.cjs",
        "default": "./src/index.js"
      }
    }
  },
  "main": "src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "lint": "eslint .",
    "test": "vitest run .",
    "test:spec:connect.function": "vitest run ./src/__tests__/connect.function.spec.js",
    "test:spec:connectFromSecretEnv.function": "vitest run ./src/__tests__/connectFromSecretEnv.function.spec.js",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "typecheck": "echo '[TODO] Convert @jaypie/mongoose to TypeScript'"
  },
  "dependencies": {
    "@jaypie/aws": "^1.1.0",
    "@jaypie/core": "^1.1.0",
    "mongoose": "^8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
