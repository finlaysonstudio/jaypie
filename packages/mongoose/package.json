{
  "name": "@jaypie/mongoose",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/finlaysonstudio/jaypie"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "lint": "eslint .",
    "test": "vitest run .",
    "test:spec:connect.function": "vitest run ./src/__tests__/connect.function.spec.ts",
    "test:spec:connectFromSecretEnv.function": "vitest run ./src/__tests__/connectFromSecretEnv.function.spec.ts",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jaypie/aws": "^1.1.0",
    "@jaypie/core": "^1.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.1",
    "mongoose": "^8.3.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "mongoose": "^8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
