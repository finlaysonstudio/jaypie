{
  "name": "@jaypie/lambda",
  "version": "1.1.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/finlaysonstudio/jaypie"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": {
        "require": "./dist/jaypie-lambda.cjs",
        "default": "./src/index.js"
      }
    }
  },
  "main": "src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "lint": "eslint .",
    "test": "vitest run .",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "test:spec:lambdaHandler": "vitest run ./src/__tests__/lambdaHandler.spec.js",
    "typecheck": "echo '[TODO] Convert @jaypie/lambda to TypeScript'"
  },
  "dependencies": {
    "@jaypie/core": "^1.1.14"
  },
  "publishConfig": {
    "access": "public"
  }
}
