  "description": "Jaypie standard application component vocabulary",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finlaysonstudio/jaypie.git"
  },
  "license": "MIT",
  "author": "Finlayson Studio",
  "sideEffects": false,
  "type": "module",
  "exports": {
