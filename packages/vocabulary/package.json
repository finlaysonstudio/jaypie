    },
    "./commander": {
      "types": "./dist/esm/commander/index.d.ts",
      "import": "./dist/esm/commander/index.js",
      "require": "./dist/cjs/commander/index.cjs"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
