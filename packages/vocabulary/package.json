    "commander": "^12.1.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vitest": "^0.5.4",
    "jest-extended": "^7.0.0",
    "rollup": "^4.12.0",
